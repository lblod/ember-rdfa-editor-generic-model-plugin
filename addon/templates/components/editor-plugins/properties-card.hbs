{{#if getAvailibleProperties.isRunning}}
  <div class="loader"><span class="u-visually-hidden">Aan het laden</span></div>
{{else}}
  {{#if classes.length}}
    <!-- <div class="modal-dialog modal-dialog--static modal-dialog--small modal-dialog--sectioned"> -->
    <div class="modal-dialog__header">
      <div class="grid">
        <div class="col--10-12">
          <p>Wilde u refereren naar</p>
        </div>
        <div class="col--2-12 u-align-right">
          <a class="link--icon--close" href="#">
            <span class="u-visually-hidden">Venster sluiten</span>
          </a>
        </div>
      </div>
    </div>
    <div class="modal-dialog__content modal-dialog__content--collapse">
      {{#each classes as |result|}}
        <h3>{{result.attributes.label}}</h3>
        <a href="#" onClick={{action "create" result}}>aanmaken</a>
        {{#if result.relationships.range.data}}
          <a href="#" onClick={{action "search"}}>zoeken</a>
        {{/if}}
      {{/each}}
    </div>
  {{else}}
    <p>Geen eigenschappen gevonden</p>
  {{/if}}
{{/if}}

{{#each errors as |error|}}
  <li class="error">! {{error}}</li>
{{/each}}
